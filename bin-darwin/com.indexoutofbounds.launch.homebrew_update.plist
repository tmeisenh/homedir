<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Label</key>
    <string>com.indexoutofbounds.launch.homebrew_update</string>

		<key>KeepAlive</key>
    <false/>

		<key>RunAtLoad</key>
    <false/>

		<key>UserName</key>
    <string>travis</string>

    <key>Program</key>
    <string>/Users/travis/bin-darwin/update_homebrew_cron.sh</string>

    <key>StandardErrorPath</key>
    <string>/Users/travis/log/update_homebrew_err.log</string>

    <key>StandardOutPath</key>
    <string>/Users/travis/log/update_homebrew.log</string>

		<key>StartCalendarInterval</key>
		<dict>
			<key>Hour</key>
			<integer>00</integer>
			<key>Minute</key>
			<integer>03</integer>
		</dict>
	</dict>
</plist>
