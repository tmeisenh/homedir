{
	"window.zoomLevel": 0,
	"explorer.openEditors.visible": 0,
	"workbench.editor.showIcons": false,
	"editor.cursorStyle": "line-thin",
	"editor.fontFamily": "Source Code Pro",
	"editor.fontSize": 18,
	"editor.fontWeight": "400",
	"editor.renderWhitespace": "none",
	"editor.minimap.enabled": false,
	"editor.minimap.maxColumn": 60,
	"files.exclude": {
		"**/karma_html": true,
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/.DS_Store": true
	},
	"editor.snippetSuggestions": "top",
	"workbench.editor.enablePreviewFromQuickOpen": false,
	"workbench.editor.enablePreview": false,
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"terminal.integrated.fontFamily": "Source Code Pro, Menlo, Monaco, 'Courier New', monospace",
	"terminal.integrated.fontSize": 12,
	"terminal.integrated.cursorStyle": "line",
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		"**/.vscode": true
	},
	"editor.autoIndent": true,
	"editor.renderIndentGuides": false,
	"workbench.colorCustomizations": {
		"editorBracketMatch.background": "#40404080",
		"editorBracketMatch.border": "#404040A0",
		"editorLineNumber.foreground": "#404060F0",
		"editorLineNumber.activeForeground": "#60607090",
		"editor.lineHighlightBorder": "#40404020",
		"editorCursor.foreground": "#909090A0"
	},
	"editor.minimap.renderCharacters": true,
	"breadcrumbs.enabled": true,
	"files.autoSave": "onFocusChange",
	"eslint.autoFixOnSave": true,
	"workbench.panel.defaultLocation": "right",
	"explorer.confirmDelete": false,
	"workbench.list.horizontalScrolling": true,
	"editor.codeLens": false,
	"editor.quickSuggestionsDelay": 50,
	"editor.colorDecorators": false,
	"extensions.ignoreRecommendations": false,
	"editor.parameterHints.enabled": false,
	"editor.cursorBlinking": "phase",
	"editor.cursorSmoothCaretAnimation": true,
	"terminal.integrated.scrollback": 10000,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"references.preferredLocation": "view",
	"editor.gotoLocation.multiple": "goto",
}